<div class="row">
  <div class="col-md-offset-11">
    <%= link_to "Upload", new_admin_upload_path, class: "btn btn-primary" %>
  </div>
</div>

<% if @uploads.blank? %>
  <p>Click the Upload button to upload some photos!</p>
<% else %>
  <div id="UploadIndex">
    <%= render 'uploads', uploads: @uploads%>
  </div>
<% end %>
