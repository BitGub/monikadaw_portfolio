<div class="row">
  <div class="col-md-offset-11">
    <%= link_to "New", new_admin_project_path, class: "btn btn-primary" %>
  </div>
</div>

<% if @projects.blank? %>
  <p>Click the New button to create a new Project </p>
<% else %>

  <% @projects.each do |project| %>

    <h2><%= link_to project.name, edit_admin_project_path(project) %> </h2>

  <% end %>
<% end %>