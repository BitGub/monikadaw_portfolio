<% if @uploads_by_year.blank? %>
  <p>Upload some photo's sexy!</p>
<% else %>
<div class="row">
  <% @uploads_by_year.sort.each do |year, uploads| %>
  <div class='col-md-4'>
    <h1><%= year %></h1>
    <% for upload in uploads %>

      <h2><%= upload.title %></h2>
      <%= link_to "Edit", edit_upload_path(upload)%>

      <%= image_tag upload.upload.url %>
    <% end %>
  </div>
  <% end %>
</div>
<% end %>